    "import numpy as np\n",
    "from scipy import stats\n",
    "import statsmodels.api as sm\n",
    "T = 30\n",
    "p = 1\n",
    "b0 = [[1.], [2.]]\n",
    "\n",
    "# Generate data\n",
    "x = stats.norm.rvs(size=[T, p])\n",
    "x = sm.add_constant(x)  # Add intercept\n",
    "y = np.dot(x, b0) + stats.norm.rvs(size=[T, 1])\n",
    "\n",
    "# Linear model\n",
    "model = sm.OLS(y, x)\n",
    "results = model.fit()\n",
    "print(results.summary())"
   ]
